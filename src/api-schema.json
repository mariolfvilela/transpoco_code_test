  "paths": {
      "/trackers": {
          "get": {
            "summary": "Get the list of trackers ordered by 'asc' | 'desc'",
            "description": "start_date: The start date for the report. Must be used together with `end_date`. \n end_date: The end date for the report. Must be used together with `start_date`.",
            "operationId": "getAll",
            "parameters": [
              {
                "name": "start_date",
                "in": "query",
                "required": false,
                "schema":{
                  "$ref": "#/components/schemas/start_date"
                }
              },
              {
                "name": "end_date",
                "in": "query",
                "required": false,
                "schema":{
                  "$ref": "#/components/schemas/end_date"
                }
              },
              {
                "name": "orderBy",
                "in": "query",
                "required": false,
                "schema":{
                  "$ref": "#/components/schemas/orderBy"
                }
              },
              {
                "name": "orderField",
                "in": "query",
                "required": false,
                "schema":{
                  "$ref": "#/components/schemas/orderField"
                }
              }
            ],
            "responses": {
              "200": {
                "description": "The list of trackers has been returned",
                "headers": {
                  "Access-Control-Allow-Origin": {
                    "schema": {
                      "type": "string"
                    }
                  },
                  "Access-Control-Allow-Methods": {
                    "schema": {
                      "type": "string"
                    }
                  },
                  "Access-Control-Allow-Headers": {
                    "schema": {
                      "type": "string"
                    }
                  }
                },
                "content": {
                  "application/json": {
                    "schema": {
                      "$ref": "#/components/schemas/TrackerPartResponse"
                    }
                  }
                }
              },
              "400": {
                "description": "Error validating the tracker data.",
                "content": {
                  "application/json": {
                    "schema": {
                      "$ref": "#/components/schemas/Error"
                    },
                    "example": {
                      "message": "Error validating tracker data",
                      "code": "400",
                      "error": "Bad Request"
                    }
                  }
                }
              },
              "404": {
                "description": "Not Found!",
                "content": {
                  "application/json": {
                    "schema": {
                      "$ref": "#/components/schemas/Error"
                    },
                    "example": {
                      "message": "User not found",
                      "code": "404",
                      "error": "Not Found"
                    }
                  }
                }
              }
            }
          }
        },
        "/trackers/{idTrackerUid}": {
          "get": {
            "summary": "Find the list of trackers ordered by ID",
            "description": "start_date: The start date for the report. Must be used together with `end_date`. \n end_date: The end date for the report. Must be used together with `start_date`.",
            "operationId": "getByTrackerUid",
            "parameters": [
              {
                "name": "start_date",
                "in": "query",
                "required": false,
                "schema":{
                  "$ref": "#/components/schemas/start_date"
                }
              },
              {
                "name": "end_date",
                "in": "query",
                "required": false,
                "schema":{
                  "$ref": "#/components/schemas/end_date"
                }
              },
              {
                "name": "idTrackerUid",
                "in": "path",
                "schema": {
                  "$ref": "#/components/schemas/idTrackerUid"
                },
                "required": true
              }
            ],
            "responses": {
              "200": {
                "description": "The list of trackers has been returned",
                "content": {
                  "application/json": {
                    "schema": {
                      "$ref": "#/components/schemas/TrackerResponse"
                    }
                  }
                }
              },
              "400": {
                "description": "Error validating the tracker data.",
                "content": {
                  "application/json": {
                    "schema": {
                      "$ref": "#/components/schemas/Error"
                    },
                    "example": {
                      "message": "Error validating tracker data",
                      "code": "400",
                      "error": "Bad Request"
                    }
                  }
                }
              },
              "404": {
                "description": "Not Found!",
                "content": {
                  "application/json": {
                    "schema": {
                      "$ref": "#/components/schemas/Error"
                    },
                    "example": {
                      "message": "User not found",
                      "code": "404",
                      "error": "Not Found"
                    }
                  }
                }
              }
            }
          }
        }
  },
