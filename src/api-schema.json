                  "in": "query",
                  "required": false,
                  "schema":{
                    "$ref": "#/components/schemas/orderField"
                  }
                }
              ],
              "responses": {
                "200": {
                  "description": "The list of trackers has been returned",
                  "content": {
                    "application/json": {
                      "schema": {
                        "$ref": "#/components/schemas/tracker"
                      }
                    }
                  }
                }
              }
            }
          }
    },
